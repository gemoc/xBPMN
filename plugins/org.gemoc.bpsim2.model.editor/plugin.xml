<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated bpsim2 -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.gemoc.bpsim2.presentation.Bpsim2ModelWizardID"
            name="%_UI_Bpsim2ModelWizard_label"
            class="org.gemoc.bpsim2.presentation.Bpsim2ModelWizard"
