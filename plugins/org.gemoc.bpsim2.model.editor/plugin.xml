      <editor
            id="org.gemoc.bpsim2.presentation.Bpsim2EditorID"
            name="%_UI_Bpsim2Editor_label"
            icon="icons/full/obj16/Bpsim2ModelFile.gif"
            extensions="bpsim,bpsim2"
            class="org.gemoc.bpsim2.presentation.Bpsim2Editor"
            contributorClass="org.gemoc.bpsim2.presentation.Bpsim2ActionBarContributor">
